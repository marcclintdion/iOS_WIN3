html, body, div, form, table, h1, h2, h3, h4, h5, h6, p, img {
	margin: 0px;
	padding: 0px;
}

body {
	background: #303030 url(/themes/new/background2.png) repeat-x;
	font-family: Verdana, "Lucida Grande", Lucida, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 140%;
}

div {
	margin: 0;
	padding: 0;
}

hr {
	background-color: #e2e1ea;
	height: 1px;
	border: 0px;
}

table a:link,
table a:visited {
	color: #00527d;
}

a:link,
a:visited {
	color: #026CB1;
	text-decoration: none;
}

a:hover, a:active {
	color: #026CB1;
	text-decoration: underline;
}

img {
	border: none;
}

/* ----- General Helper Classes ----- */
.clear {
	clear: both;
}

.hidden {
	display: none;
	float: none;
	width: 1%;
}

.float_left {
	float: left;
}

.float_right {
	float: right;
}

/* ====================== iDevGames Main Page Layout ========================*/
#pageContainer {
	width: 1156px;
	margin: 0 auto 0 auto;
}

#mainBlockBelowHeader {
}

#idgPageHeader {
	position: relative;
	height: 100px;
}

#headerImage {
	background: url(/themes/new/header_small.png) no-repeat left bottom;
	width: 600px;
	display: block;
	position: absolute;
	z-index: 100;
	top: 8px;
	left: 250px;
	height: 100px;
}

#navigationBar {
	z-index: 10;
	position: relative;
	
	width: 860px;
	height: 30px;
	margin-left:96px;
	padding: 0;
	
	text-align: center;
	line-height: 30px;
	font-size: 11px;
	font-weight: bold;
	color: #eee;
	text-shadow: 0px 1px 0px #000;
	
	background: url(/themes/new/navbar_bg.jpg) repeat-x;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
}

#navigationBar .separator {
	color: #eee;
}

#navigationBar a {
	color: #eee;
}

/* --------- Left & Right Column ---------- */
/* Imported in 'headerinclude' template. 
   This minimal amount of styling is so that
   the layout looks right before the css files
   for left and right are loaded. */
#leftColumn {
	float: left;
	width: 96px;
	min-height: 100px;
}

#rightColumn {
	float: left;
	width: 200px;
	min-height: 100px;
}

#centerColumn {
	background: rgb(250, 250, 250);
	width: 860px;
	float: left;
	position: relative;
	z-index: 9;
	min-height: 800px;
	box-shadow: 0px 0px 20px rgba(0, 0, 0, 1.0);
	border-radius: 0 0 12px 12px;
}

#footer {
	/*display: none;*/
	clear: both;
	
	z-index: 10;
	position: relative;
	
	width: 860px;
	height: 40px;
	margin-left:96px;
	padding: 0;
	
	line-height: 30px;
	font-size: 9px;
	font-weight: bold;
	text-align: center;
	color: #ccc;
}

#footer a {
	color: #ccc;
}

/* ===========================================================*/










/* ============= Bar containing simple links =================== */
/* Used on the main page, under board statistics for example */
div.links_bar {
	margin: 20px 0 20px 0;
	padding: 4px 12px 4px 12px;
	
	border: 1px solid rgba(0, 0, 0, 0.3);
	border-radius: 6px;
	background-image: -webkit-linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
	background-image:	 -moz-linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
	background-image:		  linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
	
	-webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 0.1);
	   -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 0.1);
			box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 0.1);
	
	text-shadow: 0px 1px 0px #000;
	color: white;
	text-align: right;
	
	font-size: 11px;
	line-height: 14px;
}

div.links_bar a:link,
div.links_bar a:visited {
	color: white;
	text-decoration: none;
}

div.links_bar a:hover,
div.links_bar a:active {
	color: white;
	text-decoration: underline;
}

/* Gray Style, unused. */
div.links_bar2 {
	margin: 20px 0 20px 0;
	padding: 4px 12px 4px 12px;
	
	border: 1px solid rgb(200, 200, 200);
	border-bottom-color: rgb(180, 180, 180);
	border-radius: 4px;
	
	
	background: none;
	background-image: -webkit-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
	background-image:	 -moz-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
	background-image:		  linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
	
	-webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0);
	   -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0);
			box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0);
	
	color: #666;
	text-shadow: 0 1px 0 white;
	text-align: right;
	
	font-size: 11px;
	line-height: 14px;
}

div.links_bar2 a:link,
div.links_bar2 a:visited {
	color: #666;
	text-decoration: none;
}

div.links_bar2 a:hover,
div.links_bar2 a:active {
	color: #666;
	text-decoration: underline;
}

/* =========================================================== */
















/* The "Welcome Back" panel at the top of the page */
#panel {
	background: white;
	color: #000000;
	font-size: 11px;
	border-bottom: 1px solid #D4D4D4;
	padding: 8px;
	margin-bottom: 20px;
	
	-webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.08);
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.08);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.08);
}

#panel .links {
	margin: 0;
	float: right;
}

#panel .remember_me input {
	vertical-align: middle;
	margin-top: -1px;
}

#panel .welcome_back_hack {
	color: white; /* This hides the "You last visited…" text */
}

#panel .welcome_back_hack strong {
	color: black;
}

#container {
	width: 95%;
	color: #000000;
	margin: auto auto;
	padding: 20px;
	text-align: left;
}

#content {
	width: auto !important;
}

.navigation {
	clear: both;
	background: #F2F1F0;
	
	font-size: 90%;
	
	background-image: -webkit-linear-gradient(top, #f2f1f0, #d2d1d0);
	background-image:	 -moz-linear-gradient(top, #f2f1f0, #d2d1d0);
	background-image:		  linear-gradient(top, #f2f1f0, #d2d1d0);
	
	-webkit-box-shadow: inset 0 1px 0px rgba(255, 255, 255, 0.8),  inset 0 -1px 0px rgba(255, 255, 255, 0.2);
	   -moz-box-shadow: inset 0 1px 0px rgba(255, 255, 255, 0.8),  inset 0 -1px 0px rgba(255, 255, 255, 0.2);
			box-shadow: inset 0 1px 0px rgba(255, 255, 255, 0.8),  inset 0 -1px 0px rgba(255, 255, 255, 0.2);
	
	border-bottom: 1px solid #BFBFBF;
	border-width: 1px 0;
}

.navigation ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.navigation li {
	float: left;
	
    /* Creates a top-border when an li wraps to a new line */
	-webkit-box-shadow: 0 -1px 0 rgb(189, 189, 189);
	   -moz-box-shadow: 0 -1px 0 rgb(189, 189, 189);
	        box-shadow: 0 -1px 0 rgb(189, 189, 189);
}

.navigation li a {
	height: 30px;
	line-height: 30px;
	float: left;
	margin: 0;
	margin-bottom: -1px;
	padding: 2px 15px;
	border-right:1px solid #BFBFBF;
	color: #333;
	text-decoration: none;
	text-shadow: 1px 1px 1px white;
}

.navigation li a:hover {
	color: #111;
}

.navigation li.active {
	background: #fafafa;
	background-image: -webkit-linear-gradient(top, #fdfdfd, #e0e0e0);
	background-image:	 -moz-linear-gradient(top, #fdfdfd, #e0e0e0);
	background-image:		  linear-gradient(top, #fdfdfd, #e0e0e0);
    
	-webkit-box-shadow: 0 -1px 0 rgb(189, 189, 189), inset 0 1px 0px rgba(255, 255, 255, 1.0);
	   -moz-box-shadow: 0 -1px 0 rgb(189, 189, 189), inset 0 1px 0px rgba(255, 255, 255, 1.0);
			box-shadow: 0 -1px 0 rgb(189, 189, 189), inset 0 1px 0px rgba(255, 255, 255, 1.0);
}

.navigation li.active a {
	color: #000;
	background: transparent url(/themes/nostalgia/tab-arrow.png) bottom center no-repeat;
}

/* ============ Old-School Main Table Styling ====================== */
/* Would very much like to get rid of this global styling and use classes */
table {
	color: #000000;
}

.tborder {
	background: #f3f3f3;
	
	
	/*border: 1px solid rgb(200, 200, 200);*/
	-webkit-box-shadow: -1px 0 0 rgb(200, 200, 200), 1px 0 0 rgb(200, 200, 200), 0 1px 0 rgb(200, 200, 200);
	   -moz-box-shadow: -1px 0 0 rgb(200, 200, 200), 1px 0 0 rgb(200, 200, 200), 0 1px 0 rgb(200, 200, 200);
	        box-shadow: -1px 0 0 rgb(200, 200, 200), 1px 0 0 rgb(200, 200, 200), 0 1px 0 rgb(200, 200, 200);
	
	
	border-radius: 4px;
	width: 100%;
	margin: auto auto;
	border-spacing: 0;
	
	font-size: 11px;
}

.tborder .thead:first-child {
    border-top-left-radius: 4px;
}

.tborder .thead:last-child {
    border-top-right-radius: 4px;
}

.tborder .thead {
	color: #eee;
	text-shadow: 0px 1px 0px #000;
	
	padding: 4px 8px 4px 8px;
	
	background: #2e6795;
	background-image: -webkit-linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
	background-image:	 -moz-linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
	background-image:		  linear-gradient(top, rgb(69, 108, 141), rgb(59, 89, 113));
}

.tborder .thead a:link,
.tborder .thead a:visited {
	color: #eee;
	text-decoration: none;
}

.tborder .thead a:hover,
.tborder .thead a:active {
	color: white;
	text-decoration: underline;
}

tbody tr .tcat,
tr:not(:first-child) .tcat {
    border-top: 1px solid #ccc;
}

.tborder .tcat,
.tborder .tcat a:link,
.tborder .tcat a:visited {
	color: #666;
    text-shadow: 0 1px 0 white;
}

.tborder .tcat {
	border-bottom: 1px solid #aaa;
    padding: 4px 8px 4px 8px;

    color: #666;
    text-shadow: 0 1px 0 white;

    background: none;
    background-image: -webkit-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
    background-image:    -moz-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
    background-image:         linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));

    -webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
       -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
            box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
}

.tborder tr:hover .trow1,
