/* Subheader - the table column headers row */
    .forums_depth1 .list .subheader {
        border-bottom: 1px solid #aaa;
        padding: 4px 8px 4px 8px;
    
        color: #666;
        text-shadow: 0 1px 0 white;

        background: none;
        background-image: -webkit-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
        background-image:    -moz-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
        background-image:         linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
    
        -webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
           -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
                box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1.0), inset 0 -1px 0 rgba(255, 255, 255, 0.3);
    }


    .forums_depth1 .list .subheader,
    .forums_depth1 .list .subheader a:link,
    .forums_depth1 .list .subheader a:visited {
        color: #666;
        text-shadow: 0 1px 0 white;
    }




/* Rows Separator - groups rows */
    .forums_depth1 .list .trow_sep {
        padding: 4px 8px 4px 8px;
        
        border-top: 1px solid rgba(0,0,0,0.08);
        border-bottom: 1px solid rgba(0,0,0,0.2);
        
        background: none;
        background-image: -webkit-linear-gradient(top, rgb(246, 251, 255), rgb(240, 244, 247));
        background-image:    -moz-linear-gradient(top, rgb(246, 251, 255), rgb(240, 244, 247));
        background-image:         linear-gradient(top, rgb(246, 251, 255), rgb(240, 244, 247));
        
        background-image: -webkit-linear-gradient(top, rgb(240, 244, 247), rgb(220, 224, 228));
        background-image:    -moz-linear-gradient(top, rgb(240, 244, 247), rgb(220, 224, 228));
        background-image:         linear-gradient(top, rgb(240, 244, 247), rgb(220, 224, 228));
    }


    .forums_depth1 .list .trow_sep,
    .forums_depth1 .list .trow_sep a:link,
    .forums_depth1 .list .trow_sep a:visited {
        font-weight: bold;
        font-size: 11px;
        color: rgb(150, 154, 158);
        text-shadow: 0 1px 0 white;
    }




/* Alternating row colors*/
    .forums_depth1 .list .trow1 {
        background: #f7f7f7;
        border-top: 1px solid rgba(0, 0, 0, 0.05);
    }

    .forums_depth1 .list .trow2 {
        background: #f5f5f5;
        border-top: 1px solid rgba(0, 0, 0, 0.05);
    }


    .forums_depth1 .list tbody tr:first-child .trow1 {
        border-top: none;
    }



/* Cell padding */
    .forums_depth1 .list tbody td {
        padding: 12px 8px 12px 8px;
    }
    
    .forums_depth1 .list.postlist tbody td,
    .forums_depth1 .list.threadlist tbody td {
        padding: 4px 8px 4px 8px;
    }

    .forums_depth1 .list tbody td .smalltext {
        color: #333;
    }




/* Round the bottom of the list*/
    .forums_depth1 .list tbody tr:last-child td:first-child {
        border-bottom-left-radius: 4px;
    }


    .forums_depth1 .list tbody tr:last-child td:last-child {
        border-bottom-right-radius: 4px;
    }



/*  Footer row style */
    .forums_depth1 .list tbody tr .tfoot {
        background: none;
        background-image: -webkit-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
        background-image:    -moz-linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));
        background-image:         linear-gradient(top, rgb(245, 245, 245), rgb(220, 220, 220));

        border-bottom-left-radius: 4px;
        border-bottom-right-radius: 4px;
        border-top: 1px solid rgb(200, 200, 200);

        padding: 4px;

        color: #666;
        text-shadow: 0 1px 0 white;

        -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.5);
    	   -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.5);
    	        box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.5);
    }



