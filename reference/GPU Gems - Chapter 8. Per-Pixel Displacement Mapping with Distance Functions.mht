  tanLightVec.x =3D <strong><span style=3D"color: rgb(0, 113, 188);">dot=
</span></strong>(a2v.objTangent, lightVec);

  tanLightVec.y =3D <strong><span style=3D"color: rgb(0, 113, 188);">dot=
</span></strong>(a2v.objBinormal, lightVec);

  tanLightVec.z =3D <strong><span style=3D"color: rgb(0, 113, 188);">dot=
